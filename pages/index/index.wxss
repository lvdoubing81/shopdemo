.index-swiper swiper {
  width: 750rpx;
  height: 350rpx;
}
.index-swiper swiper image {
  width: 100%;
}
.index-cate {
  display: flex;
}
.index-cate navigator {
  padding: 20rpx;
  flex: 1;
}
.index-cate navigator image {
  width: 100%;
}
.index-floor .floor-group .floor-title {
  padding: 12rpx 0;
}
.index-floor .floor-group .floor-title image {
  width: 100%;
}
.index-floor .floor-group .floor-content {
  overflow: hidden;
}
.index-floor .floor-group .floor-content navigator {
  float: left;
  width: 33.33%;
}
.index-floor .floor-group .floor-content navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 6rpx solid #ffffff;
}
.index-floor .floor-group .floor-content navigator:nth-child(2),
.index-floor .floor-group .floor-content navigator:nth-child(3) {
  border-bottom: 6rpx solid #ffffff;
}
.index-floor .floor-group .floor-content navigator image {
  width: 100%;
  height: 100%;
}
